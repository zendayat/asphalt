                if (document.getElementById("user").value != null && document.getElementById("pass").value !== null) {
                    document.getElementById("acceptance").innerHTML = "LOGIN SUCCESSFUL.WELCOME AMONG THE ELITES";
                }
                else{
                    document.getElementById("acceptance").innerHTML = "LOGIN ERROR";
                }







                function myAccept(){
                    if (document.getElementById("user").value != null){
                        if (document.getElementById("pass").value != null){
                            document.getElementById("acceptance").innerHTML = "Login Successful.Welcome Among the elites.";
                        }
                    }      
                    else{
                        document.getElementById("acceptance").innerHTML = "Failed. Please try again.";
                    }
                }









